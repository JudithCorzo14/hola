// comparacion.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;
void main()
{
	int a,b,c,mayor;
	cout<<"Ingrese a: ";
	cin>>a;
	cout<<" Ingrese b: ";
	cin>>b;
	cout<<"ingrese c: ";
	cin>>c;
	if (a>b)
		if(a>c)
			mayor=a;
		else 
		mayor=c;
	else
		if(b>c)
			mayor=b;
		else
			mayor=c;
	cout<<"El mayor es:  "<<mayor;
	getch();
}
